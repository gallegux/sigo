{
	"manifest_version": 2,
	"name": "SIGO-ESPINETE",
	"author": "F.G.R.",
	"description": "Referesca los tickets evitando que se cierre la sesi√≥n de SIGUE",
	"icons": {
		"16": "icon16.png",
		"32": "icon32.png", 
		"48": "icon48.png"
	},
	"version": "0.1",
	"permissions": [ "tabs" ],
